<div class='container'>
	<div class='row'>
		<div class='col-md-12'>
			<h1 class='text-center'>Catalogo de productos</h1>
			<hr />
			{{#each products}}
			<div class='card' style='width: 18rem;'>
				<img src='...' class='card-img-top' alt='...' />
				<div class='card-body'>
					<h5 class='card-title'>{{this.name}}</h5>
					<p class='card-text'>{{this.description}}</p>
				</div>
				<div class='card-body'>
					<p>{{this.price}}</p>
				</div>
			</div>
			{{/each}}

		</div>
	</div>